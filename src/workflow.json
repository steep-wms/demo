      "actions": [
        {
          "type": "execute",
          "service": "sort",
          "inputs": [
            {
              "id": "column1",
              "var": "col1"
            },
            {
              "id": "column2",
              "var": "col2"
            },
            {
              "id": "input_file",
              "var": "inputfile"
            }
          ],
          "outputs": [
            {
              "id": "output_file",
              "var": "sorted_file"
            }
          ]
        },
        {
          "type": "execute",
          "service": "gdal_translate",
          "inputs": [
            {
              "id": "format",
              "var": "format"
            },
            {
              "id": "input_file",
              "var": "sorted_file"
            }
          ],
          "outputs": [
            {
              "id": "output_file",
              "var": "tif_file"
            }
          ]
        },
        {
          "type": "execute",
          "service": "gdalwarp",
          "inputs": [
            {
              "id": "t_srs",
              "var": "t_srs"
            },
            {
              "id": "s_srs",
              "var": "s_srs"
            },
            {
              "id": "format",
              "var": "format"
            },
            {
              "id": "input_file",
              "var": "tif_file"
            }
          ],
          "outputs": [
            {
              "id": "output_file",
              "var": "transformed_file",
              "prefix": "tifs/"
            }
          ]
        }
      ]
    },
    {
      "type": "execute",
      "service": "qmconvert",
      "inputs": [
        {
          "id": "triangulate",
          "var": "operation"
        },
        {
          "id": "maxZoom",
          "var": "maxzoom"
        },
        {
          "id": "minZoom",
          "var": "minzoom"
        },
        {
          "id": "sourceCRS",
          "var": "t_srs"
        },
        {
          "id": "dataLocation",
          "var": "warpedfiles"
        }
      ],
      "outputs": [
        {
          "id": "output_dir",
          "var": "outputdirectory",
          "store": true
        }
      ]
    }
  ]
